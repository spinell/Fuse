{
    "$schema": "https://cmake.org/cmake/help/latest/_downloads/3e2d73bff478d88a7de0de736ba5e361/schema.json",
    "version": 10,
    "include": [ "CMakeGenericPresets.json" ],
    "configurePresets": [
        {
            "name": "linux-base",
            "$comment": [ "Base configuration fow Linux" ],
            "hidden": true,
            "inherits": [ "base-config" ],
            "environment": {},
            "cacheVariables": {}
        },
        {
            "name": "default",
            "displayName": "default",
            "description": "default",
            "inherits" :[ "linux-base" ],
            "generator": "Ninja"
        },
        {
            "name": "multi-config",
            "displayName": "Ninja Multi-Config",
            "description": "Ninja Multi-Config",
            "inherits" :[ "linux-base" ],
            "generator": "Ninja Multi-Config"
        }
    ],
    "buildPresets": [
      {
        "name": "Debug",
        "configurePreset": "multi-config",
        "configuration": "Debug"
      },
      {
        "name": "RelWithDebInfo",
        "configurePreset": "multi-config",
        "configuration": "RelWithDebInfo"
      },
      {
        "name": "Release",
        "configurePreset": "multi-config",
        "configuration": "Release"
      }
    ]
}
